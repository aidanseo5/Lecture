

function append2(xs, ys){
    return is_null(xs)
           ? ys
           : pair(head(xs) , append2(tail(xs),ys));
}
// Order of growth in time, space : theta (N)

function reverse2(lst) {
    return is_null(lst)
            ? null
            : append(reverse2(tail(lst)),list(head(lst)));
}
